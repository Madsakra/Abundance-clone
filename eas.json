{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "@project/google-services-json",
        "GOOGLE_SERVICES_INFO_PLIST": "@project/google-services-info-plist"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "@project/google-services-json",
        "GOOGLE_SERVICES_INFO_PLIST": "@project/google-services-info-plist"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
